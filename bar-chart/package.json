{
  "alfons": true,
  "author": "Jaroslav Vonr√°k",
  "dependencies": {
    "@alfons-app/pdk": "0.19.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/preset-env": "^7.28.3",
    "@babel/runtime": "^7.28.4",
    "@fluentui/react-icons": "^2.0.310",
    "expo-linear-gradient": "^13.0.2",
    "react-hook-form": "^7.65.0",
    "react-native": "^0.81.4",
    "react-native-gifted-charts": "^1.4.64",
    "react-native-web": "^0.21.1"
  },
  "devDependencies": {
    "@alfons-app/eslint-config": "0.19.0",
    "@alfons-app/sandbox": "0.19.0",
    "@jaroslav-vondrak/bar-chart": "link:",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.14",
    "@types/react": "19.1.1",
    "@types/react-dom": "19.1.7",
    "babel-plugin-module-resolver": "5.0.2",
    "babel-preset-expo": "~54.0.3",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.12",
    "react": "19.1.0",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.9.2"
  },
  "eslintConfig": {
    "extends": [
      "@alfons-app"
    ],
    "root": true
  },
  "jest": {
    "passWithNoTests": true,
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(ts|tsx|js|jsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@alfons-app/pdk$": "<rootDir>/__mocks__/@alfons-app/pdk.js",
      "^react-native$": "<rootDir>/__mocks__/react-native.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "testMatch": [
      "**/*.test.(ts|tsx|js|jsx)"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/*.d.ts",
      "!**/node_modules/**"
    ]
  },
  "license": "",
  "name": "@jaroslav-vondrak/bar-chart",
  "peerDependencies": {
    "react": "19.1.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "scope": "jaroslav-vondrak"
  },
  "scripts": {
    "lint": "eslint . --fix",
    "sandbox": "sandbox",
    "test": "jest --silent",
    "test:watch": "jest --watchAll",
    "typecheck": "tsc",
    "version:patch": "pnpm version patch --git-tag-version=false",
    "version:minor": "pnpm version minor --git-tag-version=false",
    "version:major": "pnpm version major --git-tag-version=false"
  },
  "version": "1.0.1"
}
